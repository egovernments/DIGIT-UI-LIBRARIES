{
  "name": "@egovernments/digit-ui-react-components",
  "version": "2.0.0-dev-01",
  "license": "MIT",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "example": "cd example && npm run start",
    "build": "cross-env NODE_OPTIONS='--openssl-legacy-provider' webpack --mode=production --config webpack.config.js",
    "build:dev": "cross-env NODE_OPTIONS='--openssl-legacy-provider' webpack --mode=development --config webpack.config.js",
    "build:analyze": "NODE_OPTIONS='--openssl-legacy-provider' webpack --mode=production --analyze --config webpack.config.js",
    "start": "webpack serve --mode=development --open --hot",
    "start:prod": "webpack serve --mode=production --open",
    "prepare": "yarn build",
    "predeploy": "cd example && yarn install && yarn run build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public -o dist-storybook",
    "deploy-storybook": "npm run build-storybook && surge --project dist-storybook --domain react-components-$npm_package_version.surge.sh"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.62.16",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "6.25.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.23.4",
    "@babel/plugin-transform-optional-chaining": "^7.23.4",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@storybook/addon-actions": "6.4.20",
    "@storybook/addon-essentials": "6.4.20",
    "@storybook/addon-links": "6.4.20",
    "@storybook/node-logger": "6.4.20",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.4.20",
    "@tanstack/react-query": "^5.62.16",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "core-js": "^3.38.1",
    "cross-env": "7.0.3",
    "css-loader": "^7.1.2",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-responsive": "9.0.2",
    "react-router-dom": "6.25.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^4.10.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@cyntler/react-doc-viewer": "1.10.3",
    "@egovernments/digit-ui-components": "2.0.0-dev-01",
    "@egovernments/digit-ui-svg-components": "2.0.0-dev-01",
    "@googlemaps/js-api-loader": "1.13.10",
    "@hookform/resolvers": "1.3.7",
    "ajv": "8.11.2",
    "ajv-errors": "3.0.0",
    "ajv-formats": "2.1.1",
    "json-schema-to-yup": "1.8.8",
    "react-date-range": "2.0.1",
    "react-drag-drop-files": "2.3.10",
    "react-hook-form": "6.15.8",
    "react-i18next": "15.0.0",
    "react-joyride": "2.5.5",
    "react-table": "7.7.0"
  },
  "resolutions": {
    "**/ajv": "8.11.2",
    "fast-uri": "2.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "JaganKumar <jagan.kumar@egov.org.in>",
